{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "cache": false
    },
    "lint:check": {
      "cache": false
    },
    "format": {
      "cache": false,
      "outputs": []
    },
    "format:check": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
