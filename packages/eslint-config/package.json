    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unicorn": "^55.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "eslint": "^8.57.1",
    "prettier": "^3.5.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  }
