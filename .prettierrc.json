{
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["^@workspace/*", "", "<BUILTIN_MODULES>", "", "^@/", "", "^[.]"],
  "importOrderCaseSensitive": true,
  "importOrderParserPlugins": ["typescript", "jsx"]
}
